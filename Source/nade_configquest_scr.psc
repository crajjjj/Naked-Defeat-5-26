	PlayerRef.DispelSpell(PotionDurationHealth[1])
	PlayerRef.DispelSpell(PotionDurationHealth[2])
	PlayerRef.DispelSpell(PotionDurationHealth[3])	
	SpellDurationHealthStrenght = 0
	endif
EndFunction		
	

