String Function DecimalStr(Float value, Int divider = 1)
	int i
	string s = ""

	if value<0
		s="-"
		i = Math.Ceiling(-divider*value)
	else	
		i = Math.Ceiling(divider*value)
	endif
	
	return s+i/divider+"."+i%divider
EndFunction

