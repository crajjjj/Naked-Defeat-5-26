Bool Function IsPlayerUnarmed()

	Spell spl

	Int rhand = PlayerRef.GetEquippedItemType(1)
	if rhand && (rhand != 9)
		return false
	endif
	
	Int lhand = PlayerRef.GetEquippedItemType(0)
	if lhand && (lhand != 9)
		return false
	endif
	
	if PlayerRef.IsWeaponDrawn()
		if !(rhand || lhand)
			return false
		else
			if rhand
				spl = PlayerRef.GetEquippedSpell(1)
				if spl && spl.IsHostile()
					return false
				endif
			endif
			if lhand
				spl = PlayerRef.GetEquippedSpell(1)
				if spl && spl.IsHostile()
					return false
				endif
			endif
		endif
	endif
	
	
	return true
		
EndFunction

