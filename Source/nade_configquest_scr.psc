	;	SetInstanceVolume(instanceID, 1.0)
		
		Sound.StopInstance(instanceID)
		endif 
	endif 
		
	
	if GuardsPresent 
	Debug.Trace("NAKED DEFEAT CalculatePublicExposure: GuardsPresent TRUE")
	CheckCrimePunishment()	
