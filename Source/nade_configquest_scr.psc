Bool Function DoResetQuest(Int option, Quest quest_to_reset)	;#resetquest
	Int i = quest_to_reset.GetStage()
	if (i > 0) && (i < 1000)
		quest_to_reset.SetStage(1000)
		i = quest_to_reset.GetStage()
		SetTextOptionValue(option, i)
	endif
	return (i == 1000) 
