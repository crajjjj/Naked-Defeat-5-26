	;HEARTBEATSOUND 			
	if PublicPunishment_Sounds_TOGGLE	
	
		if PublicExposure > 0 
		NymTrace("Heartbeat Start")
		Sound mySFX	

		;mySFX = (Game.GetFormFromFile(0x0004DAB0, "Skyrim.esm") As Sound)
		mySFX = (Game.GetFormFromFile(0x0004DAAF, "Skyrim.esm") As Sound)
		
