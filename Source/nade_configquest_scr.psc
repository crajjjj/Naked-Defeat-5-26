		akFormlist = Outfit_02
		elseif number == 3
		akFormlist = Outfit_03
		elseif number == 4
		akFormlist = Outfit_04
		elseif number == 5
		akFormlist = Outfit_05
		endif

		int OutfitSize = (akFormlist.GetSize()) 
		bool InventoryOpen = false
		int IdleCooldown = 0
		
		UI.CloseCustomMenu()
		

		if UI.IsMenuOpen("Main Menu") || UI.IsMenuOpen("InventoryMenu")
